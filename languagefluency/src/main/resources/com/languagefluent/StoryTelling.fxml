<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400" prefWidth="600" fx:id="library_pane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.HomeController">
    <children>
        <Label fx:id="storytellingLabel" text="Storytelling" layoutX="250" layoutY="20" style="-fx-font-size: 24px; -fx-font-weight: bold;" />

        <ImageView fx:id="logoimage" layoutX="50" layoutY="80" fitWidth="200" fitHeight="200">
            <image>
                <Image url=""/>
            </image>
        </ImageView>

        <ImageView fx:id="bookimage" layoutX="350" layoutY="80" fitWidth="200" fitHeight="200">
            <image>
                <Image url=""/>
            </image>
        </ImageView>
    </children>
    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
</AnchorPane>
